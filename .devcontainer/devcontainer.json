{
  "name": "Development Container",
  "dockerComposeFile": "docker-compose.yml",
  "service": "development",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "zsh",
        "terminal.integrated.profiles.linux": {
          "zsh": {
            "path": "/bin/zsh"
          }
        }
      }
    }
  },
  "workspaceFolder": "/workspaces/development"
}